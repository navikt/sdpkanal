<jboss-deployment-structure xmlns="urn:jboss:deployment-structure:1.2">
	
	<sub-deployment name="nav-sdp-kanal-web.war">
		
		<dependencies>
			<module name="com.sun.xml.messaging.saaj"/>
		</dependencies>
		<resources>
            <resource-root path="lib/iaik_jce_signed-${iaik.version}.jar" use-physical-code-source="true"/>
            <resource-root path="lib/iaik_xades-${iaik.xades.version}.jar" use-physical-code-source="true"/>
            <resource-root path="lib/iaik_xsect-${iaik.xsect.version}.jar" use-physical-code-source="true"/>
            <resource-root path="lib/fmsclientapi.as4-${flameas4.version}.jar" use-physical-code-source="true">
				<filter>
					<exclude path="com/ctc" />
				</filter>
			</resource-root>
        </resources>
        
        <exclusions>
            <module name="org.slf4j" />
        </exclusions>
		<exclude-subsystems>
			<subsystem name="webservices" />
		</exclude-subsystems>
    </sub-deployment>
</jboss-deployment-structure>
