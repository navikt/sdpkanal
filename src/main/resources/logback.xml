<?xml version="1.0" encoding="UTF-8" ?>
<configuration debug="true">
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%-30(%d{HH:mm:ss.SSS} %highlight(%-5level)) %-20(%magenta([%thread])) %-50(%yellow(%logger{36})) : %mdc %msg%n</pattern>
        </encoder>
    </appender>

    <root level="INFO">
        <appender-ref ref="STDOUT"/>
    </root>

    <logger name="org.apache.wss4j.dom.processor.SignatureProcessor" level="DEBUG" />
    <logger name="no.nav" level="INFO"/>
</configuration>
